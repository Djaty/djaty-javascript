{
  "extends": "airbnb",
  "rules": {
    "max-lines": [
      "warn",
      {
        "max": 500
      }
    ],
    "no-underscore-dangle": 0,
    "no-multi-assign": 0,
    "no-plusplus": 0,
    "no-mixed-operators": 0,
    "class-methods-use-this": 0,
    "no-param-reassign": 0,
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "no-continue": 0
  },
  "globals": {
    "window": true,
    "document": true,
    "Djaty": true,
    "process": true,
    "chrome": true,
    "Event": true,
    "XMLHttpRequest": true
  },
  "parserOptions": {
    "ecmaVersion": 2016
  }
}
